{
  "name": "elm-book",
  "version": "1.0.0",
  "description": "Rich documentation builder for Elm applications and packages. Based on Storybook and HexDocs.",
  "main": "index.js",
  "bin": "./bin/elm-book.js",
  "author": "Georges Boris",
  "license": "MPL-2.0",
  "homepage": "https://github.com/dtwrks/elm-book#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtwrks/elm-book.git"
  },
  "bugs": {
    "url": "https://github.com/dtwrks/elm-book/issues"
  },
  "scripts": {
    "postinstall": "elm-tooling install",
    "start": "elm-live ./src/ElmBook/UI/ElmBook.elm --port=3000 --pushstate --open -- --debug",
    "start:docs": "npx elm-doc-preview",
    "build": "npx elm make ./src/ElmBook/UI/ElmBook.elm --output=public/index.js"
  },
  "dependencies": {
    "elm-live": "^4.0.2"
  },
  "devDependencies": {
    "elm-doc-preview": "^5.0.5",
    "elm-tooling": "^1.3.0"
  }
}
